<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>disable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.0.1470.1000</AssemblyVersion>
    <FileVersion>1.0.1470.1000</FileVersion>
    <Copyright>Copyright © 2022 chibayuki@foxmail.com</Copyright>
    <Authors>chibayuki@foxmail.com</Authors>
    <Company>https://github.com/chibayuki</Company>
    <Version>1.0.1470.1000</Version>
    <ApplicationIcon>UI\Resources\AppLogo_256.ico</ApplicationIcon>
    <StartupObject>TreeOfLife.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Com" Version="20.12.25.2000" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="UI\Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="UI\Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Organism.otd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
